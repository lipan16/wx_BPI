.my{
  display: flex;
  flex-direction: column;
  height: 100vh;
}
.my-head{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 64rpx;
}
.avatar-img{
  height: 80px;
  width: 80px;
  border-radius: 50%;
}
.nickname{
  margin-top: 16px;
  font-size: 28px;
  font-weight: 600;
}
.my-content{
  flex-grow: 1;
  flex-shrink: 1;
  text-align: center;
  display: flex;
  flex-direction: column;
  padding: 0 32rpx;
}
.content{
  flex-grow: 1;
  flex-shrink: 1;
}
.copyright{
  font-size: 12px;
  margin-bottom: 8px;
}
.version{
  font-size: 12px;
  margin-bottom: 8px;
}